	"compilerOptions": {
		"target": "es2020",
		"module": "commonjs",
		"declaration": true,
		"outDir": "./lib-dist",
		"strict": true,
		"esModuleInterop": true,
		"noImplicitAny": false,
		"moduleResolution": "node",
		"sourceMap": true,
		"lib": [
			"es2015"
		]
	},
	"include": [
		"lib/**/*"
	]
}
